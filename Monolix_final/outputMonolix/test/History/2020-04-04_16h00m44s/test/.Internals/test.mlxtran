<DATAFILE>

[FILEINFO]
file = '../../../data_monolix_20200403.txt'
delimiter = tab
header = {reg_id, date, day, init_I0, init_H0, obs_id, obs, isolation, popsize, ascertainement, ascertainementmin, ascertainementmax, ascertainementsd, IDname}

[CONTENT]
day = {use=time}
init_I0 = {use=regressor}
init_H0 = {use=regressor}
obs_id = {use=observationtype}
obs = {use=observation, name={cas_confirmes_incident, hospitalisation_incident}, yname={'1', '2'}, type={discrete, discrete}}
isolation = {use=regressor}
popsize = {use=regressor}
ascertainement = {use=regressor}
IDname = {use=identifier}

<MODEL>

[INDIVIDUAL]
input = {A0_pop, omega_A0, E0_pop, omega_E0, b1_pop, omega_b1, beta_pop, Dq1_pop, omega_Dq1, betaDq_pop}

DEFINITION:
A0 = {distribution=logNormal, typical=A0_pop, sd=omega_A0}
E0 = {distribution=logNormal, typical=E0_pop, sd=omega_E0}
b1 = {distribution=logNormal, typical=b1_pop, sd=omega_b1}
beta = {distribution=normal, typical=beta_pop, no-variability}
Dq1 = {distribution=logNormal, typical=Dq1_pop, sd=omega_Dq1}
betaDq = {distribution=normal, typical=betaDq_pop, no-variability}

[LONGITUDINAL]
file = '../../../monolix_Estimation_2periode_cov_Dq.txt'

<FIT>
data = {cas_confirmes_incident, hospitalisation_incident}
model = {Y1, Y2}

<PARAMETER>
A0_pop = {value=180.8934331042406, method=MLE}
Dq1_pop = {value=1, method=MLE}
E0_pop = {value=900, method=MLE}
b1_pop = {value=0.8270287349037477, method=MLE}
betaDq_pop = {value=0, method=MLE}
beta_pop = {value=-0.3700705082865898, method=MLE}
omega_A0 = {value=1.695635478613399, method=MLE}
omega_Dq1 = {value=1, method=MLE}
omega_E0 = {value=1.103832150482672, method=MLE}
omega_b1 = {value=0.1213500514995746, method=MLE}

<MONOLIX>

[TASKS]
populationParameters()
individualParameters(method = {conditionalMean, conditionalMode })
fim(method = StochasticApproximation)
logLikelihood(method = ImportanceSampling)
plotResult(method = {outputplot, indfits, obspred, residualsscatter, residualsdistribution, parameterdistribution, covariatemodeldiagnosis, randomeffects, covariancemodeldiagnosis, saemresults })

[SETTINGS]
GLOBAL:
exportpath = '../'

